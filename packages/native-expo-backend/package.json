{
  "name": "@kikko-land/native-expo-backend",
  "version": "0.6.1",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/native-expo-backend/src/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/native-expo-backend/src/index.d.ts"
  },
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings=0",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@kikko-land/kikko": "^0.8.0",
    "@kikko-land/boono-sql": "^0.3.0",
    "@kikko-land/sql.js": "^1.6.8",
    "expo-sqlite": ">=10.3.0"
  },
  "devDependencies": {
    "@kikko-land/common-scripts": "^0.4.0",
    "@types/emscripten": "^1.39.6",
    "ts-essentials": "^9.3.0"
  }
}