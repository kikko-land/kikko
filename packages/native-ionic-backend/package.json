{
  "name": "@kikko-land/native-ionic-backend",
  "version": "0.2.0",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/native-ionic-backend/src/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/native-ionic-backend/src/index.d.ts"
  },
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings=0",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@kikko-land/kikko": "^0.2.1",
    "@kikko-land/sql": "^0.1.1",
    "@awesome-cordova-plugins/core": "^5.45.0",
    "@awesome-cordova-plugins/sqlite": "^5.45.0",
    "cordova-sqlite-storage": "^6.0.0"
  },
  "devDependencies": {
    "@kikko-land/common-scripts": "^0.1.1"
  }
}
