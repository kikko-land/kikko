{
  "name": "@kikko-land/react-native-backend",
  "version": "0.7.0",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/react-native-backend/src/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/react-native-backend/src/index.d.ts"
  },
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings=0",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@kikko-land/boono-sql": "^0.3.0",
    "@kikko-land/kikko": "^0.10.0",
    "react-native-sqlite-storage": "^6.0.1"
  },
  "devDependencies": {
    "@kikko-land/common-scripts": "^0.5.0",
    "@types/react-native-sqlite-storage": "^5.0.2"
  }
}
